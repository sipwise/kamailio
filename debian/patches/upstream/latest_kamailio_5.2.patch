+int sca_get_msg_method(sip_msg_t *msg)
+{
+	assert(msg != NULL);
+
+	if (msg->first_line.type == SIP_REQUEST) {
+		return (msg->REQ_METHOD);
+	}
+
+	return (sca_get_msg_cseq_method(msg));
